# ===========================================
# Medical Exams Parser Configuration
# ===========================================

# Primary vision model for extraction
EXTRACT_MODEL_ID=google/gemini-2.5-flash

# Model for text summarization
SUMMARIZE_MODEL_ID=google/gemini-2.5-flash

# Model for self-consistency voting
SELF_CONSISTENCY_MODEL_ID=google/gemini-2.5-flash

# Input/Output paths
INPUT_PATH=/path/to/exam/pdfs
INPUT_FILE_REGEX=.*\.pdf
OUTPUT_PATH=/path/to/output

# OpenRouter API key
OPENROUTER_API_KEY=your_api_key

# Number of extraction attempts for self-consistency
N_EXTRACTIONS=3

# Parallel workers for PDF processing
MAX_WORKERS=1
